+    code: "TG",
+    name: "Triglycerides",
+    value: 178,
+    unit: "mg/dL",
+    category: "Lipids",
+    referenceRange: "< 150",
+  },
+  {
+    id: "obs-3",
+    patientId: "patient-1",
+    code: "HDL-C",
+    name: "HDL Cholesterol",
+    value: 52,
+    unit: "mg/dL",
+    category: "Lipids",
+    referenceRange: "> 39",
+  },
+  {
+    id: "obs-4",
+    patientId: "patient-1",
+    code: "hs-CRP",
+    name: "High-Sensitivity CRP",
+    value: 1.2,
+    unit: "mg/L",
+    category: "Inflammation",
+    referenceRange: "< 3.0",
+  },
+  {
+    id: "obs-5",
+    patientId: "patient-1",
+    code: "LP-PLA2",
+    name: "Lp-PLA2 Activity",
+    value: 108,
+    unit: "nmol/min/mL",
+    category: "Inflammation",
+    referenceRange: "< 123",
+  },
+  {
+    id: "obs-6",
+    patientId: "patient-1",
+    code: "A1C",
+    name: "Hemoglobin A1c",
+    value: 5.6,
+    unit: "%",
+    category: "Metabolic",
+    referenceRange: "< 5.7",
+  },
+  {
+    id: "obs-7",
+    patientId: "patient-1",
+    code: "INS",
+    name: "Fasting Insulin",
+    value: 12,
+    unit: "µIU/mL",
+    category: "Metabolic",
+    referenceRange: "2-20",
+  },
+  {
+    id: "obs-8",
+    patientId: "patient-1",
+    code: "VitD",
+    name: "Vitamin D 25-OH",
+    value: 26,
+    unit: "ng/mL",
+    category: "Hormones",
+    referenceRange: "30-60",
+  },
+  {
+    id: "obs-9",
+    patientId: "patient-1",
+    code: "Testosterone",
+    name: "Total Testosterone",
+    value: 480,
+    unit: "ng/dL",
+    category: "Hormones",
+    referenceRange: "300-1000",
+  },
+  {
+    id: "obs-10",
+    patientId: "patient-2",
+    code: "LDL-P",
+    name: "LDL Particle Number",
+    value: 1520,
+    unit: "nmol/L",
+    category: "Lipids",
+    referenceRange: "≤ 1816",
+  },
+  {
+    id: "obs-11",
+    patientId: "patient-2",
+    code: "TG",
+    name: "Triglycerides",
+    value: 102,
+    unit: "mg/dL",
+    category: "Lipids",
+    referenceRange: "< 150",
+  },
+  {
+    id: "obs-12",
+    patientId: "patient-2",
+    code: "VitD",
+    name: "Vitamin D 25-OH",
+    value: 28,
+    unit: "ng/mL",
+    category: "Hormones",
+    referenceRange: "30-60",
+  },
+  {
+    id: "obs-13",
+    patientId: "patient-2",
+    code: "hs-CRP",
+    name: "High-Sensitivity CRP",
+    value: 0.8,
+    unit: "mg/L",
+    category: "Inflammation",
+    referenceRange: "< 3.0",
+  },
+  {
+    id: "obs-14",
+    patientId: "patient-2",
+    code: "A1C",
+    name: "Hemoglobin A1c",
+    value: 5.2,
+    unit: "%",
+    category: "Metabolic",
+    referenceRange: "< 5.7",
+  },
+];
+
+function determineFlag(code: string, value: number): ObservationFlag {
+  if (code === "LDL-P" && value > 1816) {
+    return "High";
+  }
+  if (code === "TG" && value >= 150) {
+    return "High";
+  }
+  if (code === "VitD" && value < 30) {
+    return "Low";
+  }
+  return "Normal";
+}
+
+const observations: Observation[] = observationSeeds.map((seed, index) => ({
+  id: seed.id ?? `obs-${index + 1}`,
+  flag: determineFlag(seed.code, seed.value),
+  ...seed,
+}));
+
+function clonePatient(patient: Patient): Patient {
+  return { ...patient };
+}
+
+function cloneObservation(observation: Observation): Observation {
+  return { ...observation };
+}
+
+function cloneReport(report: Report): Report {
+  return { ...report };
+}
+
+function cloneCarePlan(plan: CarePlan): CarePlan {
+  return {
+    ...plan,
+    goals: [...plan.goals],
+    interventions: [...plan.interventions],
+    monitoring: [...plan.monitoring],
+  };
+}
+
+export async function listPatients(): Promise<Patient[]> {
+  return patients.map(clonePatient);
+}
+
+export async function createPatient(input: {
+  firstName: string;
+  lastName: string;
+  dateOfBirth: string;
+  gender: "male" | "female" | "other";
+  mrn?: string;
+}): Promise<Patient> {
+  const patient: Patient = {
+    id: randomUUID(),
+    firstName: input.firstName,
+    lastName: input.lastName,
+    dateOfBirth: input.dateOfBirth,
+    gender: input.gender,
+    mrn: input.mrn ?? `MRN-${Math.floor(Math.random() * 9000 + 1000)}`,
+  };
+  patients.push(patient);
+  return clonePatient(patient);
+}
+
+export async function getPatientById(id: string): Promise<Patient | null> {
+  const patient = patients.find((item) => item.id === id);
+  return patient ? clonePatient(patient) : null;
+}
+
+export async function listObservations(patientId?: string): Promise<Observation[]> {
+  const filtered = patientId
+    ? observations.filter((observation) => observation.patientId === patientId)
+    : observations;
+  return filtered.map(cloneObservation);
+}
+
+export async function createObservation(input: {
+  patientId: string;
+  code: string;
+  name: string;
+  value: number;
+  unit: string;
+  category: ObservationCategory;
+  referenceRange?: string;
+  note?: string;
+}): Promise<Observation> {
+  const observation: Observation = {
+    id: randomUUID(),
+    patientId: input.patientId,
+    code: input.code,
+    name: input.name,
+    value: input.value,
+    unit: input.unit,
+    category: input.category,
+    referenceRange: input.referenceRange,
+    note: input.note,
+    flag: determineFlag(input.code, input.value),
+  };
+  observations.push(observation);
+  return cloneObservation(observation);
+}
+
+export async function listReports(patientId: string): Promise<Report[]> {
+  return reports
+    .filter((report) => report.patientId === patientId)
+    .map(cloneReport);
+}
+
+export async function getCarePlan(patientId: string): Promise<CarePlan | null> {
+  const plan = carePlans.find((item) => item.patientId === patientId);
+  return plan ? cloneCarePlan(plan) : null;
+}
+
+export async function getPatientDetail(id: string): Promise<PatientDetail | null> {
+  const patient = await getPatientById(id);
+  if (!patient) {
+    return null;
+  }
+  const [patientObservations, patientReports, plan] = await Promise.all([
+    listObservations(id),
+    listReports(id),
+    getCarePlan(id),
+  ]);
+
+  return {
+    patient,
+    observations: patientObservations,
+    reports: patientReports,
+    carePlan: plan,
+  };
+}
diff --git a/lab-report-app/src/lib/fhir.ts b/lab-report-app/src/lib/fhir.ts
new file mode 100644
index 0000000000000000000000000000000000000000..1d1ef86d2b7b4cc2526f7cd5cac2b7858769c62e
--- /dev/null
+++ b/lab-report-app/src/lib/fhir.ts
@@ -0,0 +1,110 @@
+import { Buffer } from "buffer";
+import { getCarePlan, getPatientById, listObservations, listReports } from "./data";
+
+export async function buildDiagnosticReport(patientId: string) {
+  const [patient, observations, reports, carePlan] = await Promise.all([
+    getPatientById(patientId),
+    listObservations(patientId),
+    listReports(patientId),
+    getCarePlan(patientId),
+  ]);
+
+  if (!patient) {
+    return null;
+  }
+
+  const issuedAt = reports[0]?.issuedAt ?? new Date().toISOString();
+
+  const contained = [
+    {
+      resourceType: "Patient",
+      id: patient.id,
+      name: [
+        {
+          given: [patient.firstName],
+          family: patient.lastName,
+        },
+      ],
+      gender: patient.gender,
+      birthDate: patient.dateOfBirth,
+      identifier: [
+        {
+          system: "http://hospital.smarthealthit.org",
+          value: patient.mrn,
+        },
+      ],
+    },
+    ...observations.map((observation) => ({
+      resourceType: "Observation",
+      id: observation.id,
+      status: "final",
+      category: [
+        {
+          text: observation.category,
+        },
+      ],
+      code: {
+        text: observation.name,
+      },
+      subject: {
+        reference: `Patient/${patient.id}`,
+      },
+      valueQuantity: {
+        value: observation.value,
+        unit: observation.unit,
+      },
+      interpretation: [
+        {
+          text: observation.flag,
+        },
+      ],
+      note: observation.note
+        ? [
+            {
+              text: observation.note,
+            },
+          ]
+        : undefined,
+    })),
+  ];
+
+  const narrativeSummary = reports
+    .map((report) => `### ${report.title}\n${report.summary}`)
+    .join("\n\n");
+
+  const planSummary = carePlan
+    ? `\n\n#### Care Plan Goals\n- ${carePlan.goals.join(
+        "\n- "
+      )}\n\n#### Interventions\n- ${carePlan.interventions.join(
+        "\n- "
+      )}\n\n#### Monitoring\n- ${carePlan.monitoring.join("\n- ")}`
+    : "";
+
+  const diagnosticReport = {
+    resourceType: "DiagnosticReport",
+    id: `report-${patient.id}`,
+    status: "final",
+    code: {
+      text: "Comprehensive Laboratory Diagnostic Report",
+    },
+    subject: {
+      reference: `Patient/${patient.id}`,
+      display: `${patient.firstName} ${patient.lastName}`,
+    },
+    effectiveDateTime: issuedAt,
+    contained,
+    result: observations.map((observation) => ({
+      reference: `#${observation.id}`,
+      display: observation.name,
+    })),
+    conclusion: reports[0]?.summary,
+    presentedForm: [
+      {
+        contentType: "text/markdown",
+        data: Buffer.from(`${narrativeSummary}${planSummary}`).toString("base64"),
+      },
+    ],
+  };
+
+  return diagnosticReport;
+}
diff --git a/lab-report-app/src/lib/models.ts b/lab-report-app/src/lib/models.ts
new file mode 100644
index 0000000000000000000000000000000000000000..8da16e2bf9d8d25c01358d9784a27b2c52d39b5e
--- /dev/null
+++ b/lab-report-app/src/lib/models.ts
@@ -0,0 +1,53 @@
+export type ObservationFlag = "High" | "Low" | "Normal";
+
+export type ObservationCategory =
+  | "Summary"
+  | "Inflammation"
+  | "Lipids"
+  | "Metabolic"
+  | "Hormones";
+
+export interface Patient {
+  id: string;
+  firstName: string;
+  lastName: string;
+  dateOfBirth: string;
+  gender: "male" | "female" | "other";
+  mrn: string;
+}
+
+export interface Observation {
+  id: string;
+  patientId: string;
+  code: string;
+  name: string;
+  value: number;
+  unit: string;
+  category: ObservationCategory;
+  referenceRange?: string;
+  flag: ObservationFlag;
+  note?: string;
+}
+
+export interface Report {
+  id: string;
+  patientId: string;
+  title: string;
+  summary: string;
+  issuedAt: string;
+}
+
+export interface CarePlan {
+  id: string;
+  patientId: string;
+  goals: string[];
+  interventions: string[];
+  monitoring: string[];
+}
+
+export interface PatientDetail {
+  patient: Patient;
+  observations: Observation[];
+  reports: Report[];
+  carePlan: CarePlan | null;
+}
