diff --git a/lab-report-app/src/app/api/observations/route.ts b/lab-report-app/src/app/api/observations/route.ts
new file mode 100644
index 0000000000000000000000000000000000000000..d117706463ffb7818da6cbf0d2ba1f48d4b950d8
--- /dev/null
+++ b/lab-report-app/src/app/api/observations/route.ts
@@ -0,0 +1,47 @@
+import { NextResponse } from "next/server";
+import { createObservation, listObservations } from "@/lib/data";
+
+export async function GET(request: Request) {
+  const { searchParams } = new URL(request.url);
+  const patientId = searchParams.get("patientId") ?? undefined;
+
+  const observations = await listObservations(patientId ?? undefined);
+  return NextResponse.json(observations);
+}
+
+export async function POST(request: Request) {
+  try {
+    const body = await request.json();
+    const { patientId, code, name, value, unit, category, referenceRange, note } =
+      body ?? {};
+
+    if (!patientId || !code || !name || value === undefined || !unit || !category) {
+      return NextResponse.json(
+        {
+          error:
+            "patientId, code, name, value, unit, and category are required to create an observation",
+        },
+        { status: 400 }
+      );
+    }
+
+    const observation = await createObservation({
+      patientId,
+      code,
+      name,
+      value: Number(value),
+      unit,
+      category,
+      referenceRange,
+      note,
+    });
+
+    return NextResponse.json(observation, { status: 201 });
+  } catch (error) {
+    console.error("Failed to create observation", error);
+    return NextResponse.json(
+      { error: "Unable to create observation" },
+      { status: 500 }
+    );
+  }
+}
diff --git a/lab-report-app/src/app/api/patients/[id]/export/route.ts b/lab-report-app/src/app/api/patients/[id]/export/route.ts
new file mode 100644
index 0000000000000000000000000000000000000000..6db249335cbe2cea365802eb8a53e021b7389cc7
--- /dev/null
+++ b/lab-report-app/src/app/api/patients/[id]/export/route.ts
@@ -0,0 +1,16 @@
+import { NextResponse } from "next/server";
+import { buildDiagnosticReport } from "@/lib/fhir";
+
+interface Params {
+  params: { id: string };
+}
+
+export async function GET(_: Request, { params }: Params) {
+  const diagnosticReport = await buildDiagnosticReport(params.id);
+
+  if (!diagnosticReport) {
+    return NextResponse.json({ error: "Patient not found" }, { status: 404 });
+  }
+
+  return NextResponse.json(diagnosticReport);
+}
diff --git a/lab-report-app/src/app/api/patients/[id]/route.ts b/lab-report-app/src/app/api/patients/[id]/route.ts
new file mode 100644
index 0000000000000000000000000000000000000000..6c3392a9e49613e81b933f7214a6d4c44d92cf8c
--- /dev/null
+++ b/lab-report-app/src/app/api/patients/[id]/route.ts
@@ -0,0 +1,16 @@
+import { NextResponse } from "next/server";
+import { getPatientDetail } from "@/lib/data";
+
+interface Params {
+  params: { id: string };
+}
+
+export async function GET(_: Request, { params }: Params) {
+  const detail = await getPatientDetail(params.id);
+
+  if (!detail) {
+    return NextResponse.json({ error: "Patient not found" }, { status: 404 });
+  }
+
+  return NextResponse.json(detail);
+}
diff --git a/lab-report-app/src/app/api/patients/route.ts b/lab-report-app/src/app/api/patients/route.ts
new file mode 100644
index 0000000000000000000000000000000000000000..feb60ba8a4b687e97e005e526b86c356afd8378a
--- /dev/null
+++ b/lab-report-app/src/app/api/patients/route.ts
@@ -0,0 +1,37 @@
+import { NextResponse } from "next/server";
+import { createPatient, listPatients } from "@/lib/data";
+
+export async function GET() {
+  const patients = await listPatients();
+  return NextResponse.json(patients);
+}
+
+export async function POST(request: Request) {
+  try {
+    const body = await request.json();
+    const { firstName, lastName, dateOfBirth, gender, mrn } = body ?? {};
+
+    if (!firstName || !lastName || !dateOfBirth || !gender) {
+      return NextResponse.json(
+        { error: "firstName, lastName, dateOfBirth, and gender are required" },
+        { status: 400 }
+      );
+    }
+
+    const patient = await createPatient({
+      firstName,
+      lastName,
+      dateOfBirth,
+      gender,
+      mrn,
+    });
+
+    return NextResponse.json(patient, { status: 201 });
+  } catch (error) {
+    console.error("Failed to create patient", error);
+    return NextResponse.json(
+      { error: "Unable to create patient" },
+      { status: 500 }
+    );
+  }
+}
