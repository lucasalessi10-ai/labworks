+++ b/lab-report-app/src/app/patients/page.tsx
@@ -0,0 +1,77 @@
+import Link from "next/link";
+import { listPatients } from "@/lib/data";
+
+function calculateAge(dateOfBirth: string) {
+  const today = new Date();
+  const birthDate = new Date(dateOfBirth);
+  let age = today.getFullYear() - birthDate.getFullYear();
+  const monthDiff = today.getMonth() - birthDate.getMonth();
+  if (monthDiff < 0 || (monthDiff === 0 && today.getDate() < birthDate.getDate())) {
+    age -= 1;
+  }
+  return age;
+}
+
+export default async function PatientsPage() {
+  const patients = await listPatients();
+
+  return (
+    <main className="mx-auto flex min-h-screen max-w-5xl flex-col gap-8 px-6 py-12">
+      <div>
+        <h1 className="text-3xl font-semibold text-slate-900">Patients</h1>
+        <p className="mt-2 text-slate-600">
+          Browse registered patients and open their full lab reports for trend
+          analysis and care planning.
+        </p>
+      </div>
+
+      <div className="overflow-hidden rounded-xl border border-slate-200 bg-white shadow-sm">
+        <table className="min-w-full divide-y divide-slate-200">
+          <thead className="bg-slate-50">
+            <tr>
+              <th scope="col" className="px-6 py-3 table-heading">
+                Patient
+              </th>
+              <th scope="col" className="px-6 py-3 table-heading">
+                MRN
+              </th>
+              <th scope="col" className="px-6 py-3 table-heading">
+                Age
+              </th>
+              <th scope="col" className="px-6 py-3 table-heading">
+                Gender
+              </th>
+              <th scope="col" className="px-6 py-3 table-heading">
+                Actions
+              </th>
+            </tr>
+          </thead>
+          <tbody className="divide-y divide-slate-200 bg-white">
+            {patients.map((patient) => (
+              <tr key={patient.id} className="hover:bg-slate-50">
+                <td className="px-6 py-4 text-sm font-medium text-slate-900">
+                  {patient.firstName} {patient.lastName}
+                </td>
+                <td className="px-6 py-4 text-sm text-slate-600">{patient.mrn}</td>
+                <td className="px-6 py-4 text-sm text-slate-600">
+                  {calculateAge(patient.dateOfBirth)}
+                </td>
+                <td className="px-6 py-4 text-sm capitalize text-slate-600">
+                  {patient.gender}
+                </td>
+                <td className="px-6 py-4 text-sm">
+                  <Link
+                    href={`/patients/${patient.id}`}
+                    className="inline-flex items-center rounded-lg border border-blue-200 px-3 py-1.5 text-blue-600 transition hover:border-blue-300 hover:bg-blue-50"
+                  >
+                    View details
+                  </Link>
+                </td>
+              </tr>
+            ))}
+          </tbody>
+        </table>
+      </div>
+    </main>
+  );
+}
diff --git a/lab-report-app/src/components/patient-tabs.tsx b/lab-report-app/src/components/patient-tabs.tsx
new file mode 100644
index 0000000000000000000000000000000000000000..6dfbced98b1da0a5263ca39fa533643cdfab6b4b
--- /dev/null
+++ b/lab-report-app/src/components/patient-tabs.tsx
@@ -0,0 +1,280 @@
+"use client";
+
+import Link from "next/link";
+import { useMemo, useState } from "react";
+import type { Observation, PatientDetail } from "@/lib/models";
+
+const tabs = [
+  { id: "Summary", label: "Summary" },
+  { id: "Inflammation", label: "Inflammation" },
+  { id: "Lipids", label: "Lipids" },
+  { id: "Metabolic", label: "Metabolic" },
+  { id: "Hormones", label: "Hormones" },
+  { id: "CarePlan", label: "Care Plan" },
+] as const;
+
+type TabId = (typeof tabs)[number]["id"];
+
+function getAge(dateOfBirth: string) {
+  const today = new Date();
+  const birthDate = new Date(dateOfBirth);
+  let age = today.getFullYear() - birthDate.getFullYear();
+  const monthDiff = today.getMonth() - birthDate.getMonth();
+  if (monthDiff < 0 || (monthDiff === 0 && today.getDate() < birthDate.getDate())) {
+    age -= 1;
+  }
+  return age;
+}
+
+function FlagBadge({ flag }: { flag: Observation["flag"] }) {
+  const colorMap = {
+    High: "border-flag-high/20 bg-flag-high/10 text-flag-high",
+    Low: "border-flag-low/20 bg-flag-low/10 text-flag-low",
+    Normal: "border-flag-normal/20 bg-flag-normal/10 text-flag-normal",
+  } as const;
+
+  return (
+    <span
+      className={`inline-flex items-center rounded-full border px-3 py-1 text-xs font-semibold uppercase ${colorMap[flag]}`}
+    >
+      {flag}
+    </span>
+  );
+}
+
+function ObservationTable({ observations }: { observations: Observation[] }) {
+  if (observations.length === 0) {
+    return (
+      <div className="card">
+        <p className="text-sm text-slate-600">No observations in this category.</p>
+      </div>
+    );
+  }
+
+  return (
+    <div className="overflow-hidden rounded-xl border border-slate-200 bg-white shadow-sm">
+      <table className="min-w-full divide-y divide-slate-200">
+        <thead className="bg-slate-50">
+          <tr>
+            <th scope="col" className="px-6 py-3 text-left text-xs font-semibold uppercase tracking-wide text-slate-500">
+              Marker
+            </th>
+            <th scope="col" className="px-6 py-3 text-left text-xs font-semibold uppercase tracking-wide text-slate-500">
+              Value
+            </th>
+            <th scope="col" className="px-6 py-3 text-left text-xs font-semibold uppercase tracking-wide text-slate-500">
+              Reference
+            </th>
+            <th scope="col" className="px-6 py-3 text-left text-xs font-semibold uppercase tracking-wide text-slate-500">
+              Flag
+            </th>
+          </tr>
+        </thead>
+        <tbody className="divide-y divide-slate-200 bg-white">
+          {observations.map((observation) => (
+            <tr key={observation.id} className="hover:bg-slate-50">
+              <td className="px-6 py-4 text-sm font-medium text-slate-900">
+                <div>{observation.name}</div>
+                <p className="text-xs text-slate-500">{observation.code}</p>
+              </td>
+              <td className="px-6 py-4 text-sm text-slate-600">
+                {observation.value} {observation.unit}
+              </td>
+              <td className="px-6 py-4 text-sm text-slate-600">
+                {observation.referenceRange ?? "N/A"}
+              </td>
+              <td className="px-6 py-4 text-sm">
+                <FlagBadge flag={observation.flag} />
+              </td>
+            </tr>
+          ))}
+        </tbody>
+      </table>
+    </div>
+  );
+}
+
+export function PatientTabs({ detail }: { detail: PatientDetail }) {
+  const [activeTab, setActiveTab] = useState<TabId>("Summary");
+
+  const groupedObservations = useMemo(() => {
+    return detail.observations.reduce<Record<string, Observation[]>>((acc, observation) => {
+      if (!acc[observation.category]) {
+        acc[observation.category] = [];
+      }
+      acc[observation.category].push(observation);
+      return acc;
+    }, {});
+  }, [detail.observations]);
+
+  const flagged = useMemo(
+    () => detail.observations.filter((observation) => observation.flag !== "Normal"),
+    [detail.observations]
+  );
+
+  const latestReport = detail.reports[0] ?? null;
+
+  return (
+    <div className="space-y-6">
+      <div className="flex flex-wrap gap-2 border-b border-slate-200 pb-2">
+        {tabs.map((tab) => (
+          <button
+            key={tab.id}
+            type="button"
+            onClick={() => setActiveTab(tab.id)}
+            className={`rounded-t-lg border-b-2 px-4 py-2 text-sm font-medium transition focus:outline-none focus-visible:ring-2 focus-visible:ring-blue-500 focus-visible:ring-offset-2 focus-visible:ring-offset-slate-100 ${
+              activeTab === tab.id
+                ? "border-blue-600 bg-white text-blue-600"
+                : "border-transparent text-slate-500 hover:text-slate-700"
+            }`}
+          >
+            {tab.label}
+          </button>
+        ))}
+      </div>
+
+      {activeTab === "Summary" && (
+        <div className="grid gap-6 lg:grid-cols-2">
+          <section className="card space-y-3">
+            <h2 className="text-lg font-semibold text-slate-900">Patient profile</h2>
+            <dl className="grid grid-cols-2 gap-3 text-sm text-slate-600">
+              <div>
+                <dt className="font-medium text-slate-500">Name</dt>
+                <dd>
+                  {detail.patient.firstName} {detail.patient.lastName}
+                </dd>
+              </div>
+              <div>
+                <dt className="font-medium text-slate-500">MRN</dt>
+                <dd>{detail.patient.mrn}</dd>
+              </div>
+              <div>
+                <dt className="font-medium text-slate-500">Date of birth</dt>
+                <dd>{detail.patient.dateOfBirth}</dd>
+              </div>
+              <div>
+                <dt className="font-medium text-slate-500">Age</dt>
+                <dd>{getAge(detail.patient.dateOfBirth)}</dd>
+              </div>
+              <div>
+                <dt className="font-medium text-slate-500">Gender</dt>
+                <dd className="capitalize">{detail.patient.gender}</dd>
+              </div>
+            </dl>
+            <Link
+              href={`/api/patients/${detail.patient.id}/export`}
+              className="inline-flex w-fit items-center gap-2 rounded-lg border border-slate-200 px-4 py-2 text-sm font-medium text-slate-700 transition hover:border-blue-200 hover:text-blue-600"
+            >
+              Download FHIR Diagnostic Report
+            </Link>
+          </section>
+
+          <section className="card space-y-3">
+            <h2 className="text-lg font-semibold text-slate-900">Flagged observations</h2>
+            {flagged.length === 0 ? (
+              <p className="text-sm text-slate-600">
+                No markers are currently outside the optimal range.
+              </p>
+            ) : (
+              <ul className="space-y-3">
+                {flagged.map((observation) => (
+                  <li key={observation.id} className="flex items-center justify-between gap-3">
+                    <div>
+                      <p className="text-sm font-medium text-slate-900">
+                        {observation.name}
+                      </p>
+                      <p className="text-xs text-slate-500">
+                        {observation.value} {observation.unit} · Target {observation.referenceRange ?? "n/a"}
+                      </p>
+                    </div>
+                    <FlagBadge flag={observation.flag} />
+                  </li>
+                ))}
+              </ul>
+            )}
+          </section>
+
+          <section className="card space-y-3 lg:col-span-2">
+            <h2 className="text-lg font-semibold text-slate-900">Latest report</h2>
+            {latestReport ? (
+              <div className="space-y-2 text-sm text-slate-700">
+                <p className="font-semibold text-slate-900">{latestReport.title}</p>
+                <p className="text-xs text-slate-500">Issued {new Date(latestReport.issuedAt).toLocaleDateString()}</p>
+                <p>{latestReport.summary}</p>
+              </div>
+            ) : (
+              <p className="text-sm text-slate-600">No reports are available.</p>
+            )}
+          </section>
+        </div>
+      )}
+
+      {activeTab === "Inflammation" && (
+        <ObservationTable observations={groupedObservations["Inflammation"] ?? []} />
+      )}
+
+      {activeTab === "Lipids" && (
+        <ObservationTable observations={groupedObservations["Lipids"] ?? []} />
+      )}
+
+      {activeTab === "Metabolic" && (
+        <ObservationTable observations={groupedObservations["Metabolic"] ?? []} />
+      )}
+
+      {activeTab === "Hormones" && (
+        <ObservationTable observations={groupedObservations["Hormones"] ?? []} />
+      )}
+
+      {activeTab === "CarePlan" && (
+        <section className="card space-y-4">
+          <h2 className="text-lg font-semibold text-slate-900">Care plan</h2>
+          {detail.carePlan ? (
+            <div className="grid gap-4 lg:grid-cols-3">
+              <div>
+                <h3 className="text-sm font-semibold uppercase tracking-wide text-slate-500">
+                  Goals
+                </h3>
+                <ul className="mt-2 space-y-2 text-sm text-slate-700">
+                  {detail.carePlan.goals.map((goal, index) => (
+                    <li key={index} className="flex gap-2">
+                      <span className="mt-1 h-2 w-2 rounded-full bg-blue-500" aria-hidden />
+                      <span>{goal}</span>
+                    </li>
+                  ))}
+                </ul>
+              </div>
+              <div>
+                <h3 className="text-sm font-semibold uppercase tracking-wide text-slate-500">
+                  Interventions
+                </h3>
+                <ul className="mt-2 space-y-2 text-sm text-slate-700">
+                  {detail.carePlan.interventions.map((item, index) => (
+                    <li key={index} className="flex gap-2">
+                      <span className="mt-1 h-2 w-2 rounded-full bg-emerald-500" aria-hidden />
+                      <span>{item}</span>
+                    </li>
+                  ))}
+                </ul>
+              </div>
+              <div>
+                <h3 className="text-sm font-semibold uppercase tracking-wide text-slate-500">
+                  Monitoring
+                </h3>
+                <ul className="mt-2 space-y-2 text-sm text-slate-700">
+                  {detail.carePlan.monitoring.map((item, index) => (
+                    <li key={index} className="flex gap-2">
+                      <span className="mt-1 h-2 w-2 rounded-full bg-amber-500" aria-hidden />
+                      <span>{item}</span>
+                    </li>
+                  ))}
+                </ul>
+              </div>
+            </div>
+          ) : (
+            <p className="text-sm text-slate-600">No care plan recorded.</p>
+          )}
+        </section>
+      )}
+    </div>
+  );
+}
