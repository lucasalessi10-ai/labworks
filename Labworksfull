@@ -0,0 +1,40 @@
+import { notFound } from "next/navigation";
+import Link from "next/link";
+import { PatientTabs } from "@/components/patient-tabs";
+import { getPatientDetail } from "@/lib/data";
+
+interface PatientDetailPageProps {
+  params: { id: string };
+}
+
+export default async function PatientDetailPage({ params }: PatientDetailPageProps) {
+  const detail = await getPatientDetail(params.id);
+
+  if (!detail) {
+    notFound();
+  }
+
+  return (
+    <main className="mx-auto flex min-h-screen max-w-5xl flex-col gap-8 px-6 py-12">
+      <div className="flex items-center justify-between gap-4">
+        <div>
+          <h1 className="text-3xl font-semibold text-slate-900">
+            {detail.patient.firstName} {detail.patient.lastName}
+          </h1>
+          <p className="mt-2 text-slate-600">
+            Review longitudinal biomarkers, flagged risks, and the curated care
+            plan for this patient.
+          </p>
+        </div>
+        <Link
+          href="/patients"
+          className="hidden rounded-lg border border-slate-200 px-4 py-2 text-sm font-medium text-slate-600 transition hover:border-blue-200 hover:text-blue-600 md:inline-flex"
+        >
+          Back to patients
+        </Link>
+      </div>
+
+      <PatientTabs detail={detail} />
+    </main>
+  );
+}
diff --git a/lab-report-app/src/app/patients/page.tsx b/lab-report-app/src/app/patients/page.tsx
new file mode 100644
index 0000000000000000000000000000000000000000..6bb2833dafc2a20f5717912163e5ca1e1d153fc4
